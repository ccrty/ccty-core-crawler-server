<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ccty.noah.domain.mapper.CommentMapper">

    <!-- 批量插入评价信息 -->
    <insert id="insertComments" parameterType="java.util.List">
        insert into ccty_comment(aid,content,source,creator) values
        <foreach collection="list" item="comment" separator=",">
            (
              #{comment.aid},#{comment.content},#{comment.commentSource},#{comment.creator}
            )
        </foreach>
    </insert>


</mapper>